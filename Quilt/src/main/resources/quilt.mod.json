{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "${mod_id}",
    "version": "${version}",
    "icon": "assets/${mod_id}/icon.png",
    "metadata": {
      "name": "${mod_name}",
      "description": "${mod_description}",
      "contributors": {
        "${mod_author}": "Owner"
      },
      "contact": {
        "homepage": "${mod_homepage}",
        "issues": "${mod_issue_tracker}",
        "sources": "${mod_source}"
      },
      "license": [
        {
          "name": "All Rights Reserved",
          "id": "ARR (Assets)",
          "url": "https://en.wikipedia.org/wiki/All_rights_reserved",
          "description": "License for the art in this module"
        },
        {
          "name": "${mod_license_long}",
          "id": "${mod_license_id} (Code)",
          "url": "https://github.com/tophatcats-mods/kiris-lib/blob/dev/LICENSE.txt",
          "description": "License for the code in this mod"
        }
      ]
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": "dev.tophatcat.kirislib.KirisLibQuilt"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=${quilt_loader_version}"
      },
      {
        "id": "minecraft",
        "versions": ">=${minecraft_version}"
      }
    ]
  },
  "modmenu": {
    "links": {
      "modmenu.discord": "${mod_discord}",
      "modmenu.twitter": "${mod_twitter}"
    }
  }
}
